{
  "name": "ramp-demoqa-assessment",
  "version": "1.0.0",
  "description": "Comprehensive QA Assessment covering UI and API testing using Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "test:ui": "cypress run --spec 'cypress/e2e/ui/**/*'",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*'",
    "test:all": "cypress run",
    "test:report": "cypress run --reporter mochawesome",
    "lint": "eslint cypress/**/*.js",
    "format": "prettier --write cypress/**/*.js"
  },
  "keywords": [
    "cypress",
    "testing",
    "qa",
    "automation",
    "ui",
    "api",
    "javascript"
  ],
  "author": "QA Assessment",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "cypress": "^14.5.2",
    "eslint": "^8.55.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^3.1.0"
  }
}
